<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.city_portal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".BookDoctorAppointment"></activity>
        <activity android:name=".Receipt" />
        <activity android:name=".BookBeautyAppointment" />
        <activity android:name=".Beautician" />
        <activity android:name=".Doctors" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VisitRestaurantActivity" />
        <activity android:name=".VisitHospitalActivity" />
        <activity android:name=".VisitEducationActivity" />
        <activity android:name=".VisitActivity" />
        <activity android:name=".TestActivity" />
        <activity android:name=".signup" />
        <activity android:name=".login" />
        <activity android:name=".restaurant" />
        <activity android:name=".beauty" />
        <activity android:name=".education" />
        <activity android:name=".hospital" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>